namespace HW_3;

using HW_3::ExtraInfo.Food;
using HW_3::ExtraInfo.FridgeOwner;


entity Fridge {
    @Comment: 'Id of Fridge'
    key frid : String(4);
    @Comment: 'Brand's name'
    bname : String(100);
		@Comment: 'Capacity'
		cap: String(5);
		

    toFood : Association[*] to Food on toFood.frid = frid;
    toOwner : Association[1] to FridgeOwner on toOwner.frid = frid;
};