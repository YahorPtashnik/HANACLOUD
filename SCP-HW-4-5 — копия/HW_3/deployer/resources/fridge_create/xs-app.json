{  
   "welcomeFile":"/index.html",
   "authenticationMethod":"route",
   "routes":[  
      {  
         "source":"^(.*)$",
         "target":"$1",
         "service":"html5-apps-repo-rt",
         "csrfProtection":true,
         "authenticationType":"xsuaa",
         "scope":{  
            "GET":[  
               "$XSAPPNAME.HW_3.view"
            ],
            "POST":[  
               "$XSAPPNAME.HW_3.edit"
            ],
            "PUT":[  
               "$XSAPPNAME.HW_3.edit"
            ],
            "DELETE":[  
               "$XSAPPNAME.HW_3.edit"
            ],
            "default":"$XSAPPNAME.HW_3.view"
         }
      }
   ]
}